package paella {	import paella.JavascriptTrace;import paella.External;public class JavascriptInterface {	var _playerId:String;		public function JavascriptInterface() {	}		public function get playerId():String {		return this._playerId;	}		public function set playerId(id:String):void {		this._playerId = id;	}		public function sendEvent(eventName:String, eventValues:String):void {		var eventData = "{ source:'" +  this._playerId + "', eventName:'" + eventName + "', values:'" + eventValues + "'}";		External.call("paella.events.trigger(paella.events.flashVideoEvent," + eventData + ")");//		JavascriptTrace.debug("send event: " + eventName + ", values: " + eventValues);	}}	}